# To include this file, run the following command:
# git config --global include.path "C:\projects\settings-and-config\.gitconfig"
[http]
	sslVerify = false
[push]
	default = current
[core]
	editor = code-insiders --wait
	excludesfile = C:/projects/settings-and-config/.gitignore_global
[rebase]
	autoStash = true
[merge]
	tool = kdiff3
[mergetool "kdiff3"]
	path = C:/Program Files/KDiff3/kdiff3.exe
[mergetool]
	keepBackup = false
[diff]
	guitool = kdiff3
[difftool "kdiff3"]
	path = C:/Program Files/KDiff3/kdiff3.exe
[user]
	name = Massimo Hamilton
	email = massimo.hamilton@outlook.com
[credential]
	helper = manager
